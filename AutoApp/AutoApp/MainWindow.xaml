<Window x:Class="AutoApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AutoApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Closed="Window_Closed">
    <StackPanel VerticalAlignment="Center">
        <Button x:Name="LoginBtn" Content="进入网址" Height="30" Width="75" Click="LoginBtn_Click"/>
        <Button x:Name="StartBtn" Content="开始运行" Height="30" Width="75" Click="StartBtn_Click"/>
    </StackPanel>
</Window>
